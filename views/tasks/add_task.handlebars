        <script>
            $('.date-picker').datepicker({
                rtl: App.isRTL(),
                orientation: "left",
                autoclose: true
            });

            $('#addTaskBtn').click(function(){
                $('.response').show();
                $('.response strong').text('Adding Task ...');

                var data = {};
                data.subject = $('#subject').val();
                data.dueDate = $('#dueDate').val();
                data.comment = $('#comment').val();
                data.UserAccount = $('#UserAccount').val();
                data.userAccountTeam = $('#userAccountTeam').val();
                data.assigned = $('#assigned').val();
                data.priority = $('#priority').val();
                data.status = $('#status').val();
                data.created_by = $('#user_id').val();

                $.ajax({
                    url: '/admin/add_task',
                    method: 'POST',
                    contentType: 'application/json',
                    data: JSON.stringify(data),
                    success: function(response){
                        $('#tasksuccess').show();
                    }
                });
                $('.response').hide();
                $('.tasksuccess').show();
                $('#addTasks')[0].reset();
            });

            $(document).ready(function(){
                $.getJSON( '/user/api', function( data ) {
                    var path = data.path;
                    $('#created_by').val(data.fullname);
                    $('#user_id').val(data._id);
                });
                

                $.getJSON( '/admin/accounts/api', function( dataAccount ){
                    $('#UserAccount').html('<option value="0">--- Select Account ---</option>');

                    for (var i = 0; i < dataAccount.length; i++) { 
                        var account_name = dataAccount[i].account_name;
                        var account_id = dataAccount[i]._id;
                        $('#UserAccount').append('<option value="'+ account_id +'" data-id="'+ account_id +'">'+ account_name +'</option>');   
                    }

                   $('#UserAccount').change(function(){
                       if($(this).val() != 0){
                           $('#selectuserteam').show();
                           var selected = $(this).find('option:selected');
                           var selectedAccount = selected.data('id');
                           $.getJSON('/admin/accounts/show_accounts/team/api/' + selectedAccount, function(accountTeam){
                                $('#userAccountTeam').html('<option value="0">--- Select Campaign ---</option>');
                                for (var i = 0; i < accountTeam.length; i++) { 
                                    var team_name = accountTeam[i].team_name;
                                    var team_id = accountTeam[i]._id;
                                    $('#userAccountTeam').append('<option value="'+ team_id +'" data-id="'+ team_id +'">'+ team_name +'</option>');  
                                }
                           });
                       } else {
                           $('#selectuserteam').hide();
                       }
                   });

                   $('#userAccountTeam').change(function(){
                        if ( $(this).val() != 0 ) {
                            $('#selectuser').show();
                            var selected = $(this).find('option:selected');
                            var selectedteam = selected.data('id');
                            $.getJSON('/admin/user/team/api/' + selectedteam, function(userData){
                                 $('#assigned').html('<option value="0">--- Select Campaign ---</option>');
                                 for(var i = 0 ; i < userData.length; i++){
                                     var fullname = userData[i].fullname;
                                     var user_id = userData[i]._id;
                                     $('#assigned').append('<option value="'+ user_id +'" data-id="'+ user_id +'">'+ fullname +'</option>');  
                                 }
                            });
                        } else {
                            $('#selectuser').hide();
                        }   
                   });
                });

            });
        </script>
