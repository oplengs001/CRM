        <script type="text/javascript">
            $(document).ready(function(){

                var pathArray = window.location.pathname.split( '/' );
                var account_id = pathArray[4];

               $.getJSON('/admin/accounts/api/' + account_id, function(queryResult) {
                  var account_id = queryResult._id;
                  var account_name = queryResult.account_name;
                  var account_owner = queryResult.account_owner;
                  var account_type = queryResult.account_type;
                  var description = queryResult.description;
                  var industry = queryResult.industry;
                  var phone_number = queryResult.phone_number;
                  var website_url = queryResult.website_url;

                  $('#accountname').val(account_name);
                  $('#account_type').val(account_type);
                  $('#website_url').val(website_url);
                  $('#description').val(description);
                  $('#account_owner').val(account_owner);
                  $('#phone_number').val(phone_number);
                  $('#industry').val(industry);

                });

               $('#update').click(function(){
                    var newAccountName = $('#accountname').val();
                    var accounttype = $('#account_type').val();
                    var websiteurl = $('#website_url').val();
                    var NewDescription = $('#description').val();
                    var accountowner = $('#account_owner').val();
                    var phonenumber = $('#phone_number').val();
                    var NewIndustry = $('#industry').val();
                    $.ajax({
                        url: '/admin/accounts/api/' + account_id,
                        method: 'PUT',
                        contentType: 'application/json',
                        data: JSON.stringify(
                        { 
                            account_name : newAccountName,
                            account_type : accounttype,
                            website_url : websiteurl,
                            description : NewDescription,
                            account_owner : accountowner,
                            phone_number : phonenumber,
                            industry : NewIndustry
                        }
                        ),
                        success:function(response){
                            console.log(response);
                            console.log('success');
                            window.location.href = '/admin/accounts';
                        },
                        error:function(response){
                            console.log("error");
                        }
                    })
               });
            });
        </script>
